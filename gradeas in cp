#include<stdio.h>
int main()
{
  int score;
  char grade;
  
  printf("Enter the score(0-100): ");
  scanf(%d, &score);
  return 0;
}

if (score>=85 && score<=100);
  grade = 'A';
if (score>=70 && score<=84);
  grade = 'B';
if (score>=55 && score<=69);
  grade = 'C';
if (score>=40 && score<=54);
  grade = 'D';
else if  (score<40);
  grade ='F';
